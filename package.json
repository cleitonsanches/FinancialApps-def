{
  "name": "projeto-financial",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=apps/api\" \"npm run dev --workspace=apps/web\"",
    "dev:api": "npm run dev --workspace=apps/api",
    "dev:web": "npm run dev --workspace=apps/web",
    "auto-save": "node auto-save.js",
    "auto-save:start": "start /B node auto-save.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
